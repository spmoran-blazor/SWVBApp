<MudStack Spacing="3" Row="true" Class="pb-2">
    <MudField Variant="Variant.Text">Arthur Fonzarelli - Unconfirmed</MudField>
    <MudIconButton OnClick="@ToggleValue" Variant="Variant.Text" DisableElevation="true" Icon="@Icons.Filled.ThumbUp"></MudIconButton>
    <MudIconButton OnClick="@ToggleValue" Variant="Variant.Text" DisableElevation="true" Icon="@Icons.Filled.ThumbDown"></MudIconButton>
</MudStack>

@code {
    public bool SwitchValue { get; set; } = true;
    public bool ReadOnly { get; set; } = true;

    void ToggleValue()
    {
        SwitchValue = !SwitchValue;
    }
}