{
  "routes": [
    {
      "route": "/",
      "allowedRoles": [ "anonymous" ] 
    },
    {
      "route": "/eventregister",
      "allowedRoles": [ "anonymous" ]
    },
    {
      "route": "/profile",
      "allowedRoles": [ "anonymous" ]
    }
  ],
  "globalHeaders": {
    "Cache-Control": "no-cache"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/img/",
      "{png,jpg,gif}",
      "/css/"
    ]
  }
}
